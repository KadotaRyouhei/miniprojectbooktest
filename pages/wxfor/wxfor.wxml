<view class="container">
<image src="../images/year_pig.jpg" mode='aspectFill'></image>
  <block wx:for="{{users}}">
  <template is="userTemp" data="{{item}}"/>
  </block>
</view>


<template name="userTemp">
<view class="user">
<view>姓名：{{item.name}}</view>
<view>性别：{{item.sex}}</view>
<view>年龄：{{item.age}}</view>
</view>
</template>